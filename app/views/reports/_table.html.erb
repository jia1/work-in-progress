<div class="row">
  <table>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Function</th>
      <th>Action</th>
    </tr>
   
    <% @reports.reverse.each do |report| %>
      <tr>
        <td><%= report.date %></td>
        <td><%= report.title %></td>
        <td><%= report.function %></td>
        <td>
          <%= link_to 'More', report_path(report) %><br>
          <%= link_to 'Edit', edit_report_path(report) %>
        </td>
      </tr>
    <% end %>
  </table>
</div>